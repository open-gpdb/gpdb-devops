#!/usr/bin/make -f
DH_VERBOSE = 1
PXF_HOME ?= debian/build/

%:
	dh $@

override_dh_auto_configure:
	echo "Skipping autoconfigure"

override_dh_auto_build:
	make -C server stage-jdbc-drivers

override_dh_auto_install:
	make -C server stage-jdbc-drivers
	mkdir -p "$(PXF_HOME)"/lib
	cp -R build/stage/lib/* "$(PXF_HOME)"/lib

override_dh_auto_test:
	echo "Skipping auto test"

override_dh_auto_clean:
	echo "Skipping auto clean"

override_dh_strip_nondeterminism:
	echo "Skipping dh_strip_nondeterminism"
